{
  "name": "neec-backend",
  "version": "1.0.0",
  "description": "Backend code for Neec Aplication",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon --exec \"node --loader ts-node/esm\" index.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_ENV=development NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint",
    "docs": "node tools/serve-docs.js",
    "security:audit": "bash scripts/security-audit.sh",
    "type-check": "tsc --noEmit",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "npm run typeorm -- migration:generate -d db/ormconfig.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d db/ormconfig.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d db/ormconfig.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d db/ormconfig.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d db/ormconfig.ts",
    "generate:endpoint": "node scripts/generate-endpoint.js",
    "generate": "node scripts/generate-endpoint.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ismaeltorresh/neec-backend.git"
  },
  "author": "@ismaeltorresh",
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@sentry/node": "^8.32.0",
    "@sentry/profiling-node": "^8.32.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.0.0",
    "mysql2": "^3.16.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.28",
    "yaml": "^2.3.4",
    "zod": "^4.2.1"
  }
}
